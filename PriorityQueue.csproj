<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
	  <LangVersion>preview</LangVersion>
	  <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
	  <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	  <OutputType>Library</OutputType>
	  <PackageId>UskokPriorityQueue</PackageId>
	  <Title>Priority Queue</Title>
	  <Version>1.0.0</Version>
	  <Authors>Vuk Uskokovic</Authors>
	  <Description>A simple class that helps create a thread safe queue with priority.</Description>
	  <RepositoryType>git</RepositoryType>
	  <RepositoryUrl>https://github.com/vukuskokovic/priority-queue</RepositoryUrl>
	  <PackageTags>Queue, Priority, List, Collection, Array</PackageTags>
  </PropertyGroup>

</Project>
